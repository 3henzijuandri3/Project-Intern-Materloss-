		{
			"character": "character-1630073371.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Halo nak Marshall.....\nAda perlu apa ke sini ?\nMau mengambil obat untuk ibumu ya ?"
		},
		{
			"action": "join",
			"character": "character-1630072959.json",
			"event_id": "dialogic_002",
			"mirror": false,
			"portrait": "Default",
			"position": {
